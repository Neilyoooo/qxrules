{
  "id": "lixinzai.app.sub",
  "name": "lixinzai 应用订阅",
  "author": "@lixinzai",
  "icon": "https://avatars.githubusercontent.com/u/59134415",
  "repo": "https://github.com/lixinzai",
  "apps": [
    {
      "id": "JDWSKEY",
      "name": "京东WSKEY管理",
      "author": "@lixinzai",
      "descs_html": [
        "用于自动抓取京东WSKEY可选发送tgbot"
      ],
      "icons": [
        "https://raw.githubusercontent.com/lixinzai/PNG/main/jdjoy.png",
        "https://raw.githubusercontent.com/lixinzai/PNG/main/jdjoy.png"
      ],
      "keys": [
        "WSKEY-AUTOUPLOAD",
        "WSKEY-TG-BOTTOKEN",
        "WSKEY-TG-USERID",
        "wskeyList"
      ],
      "settings": [
        {
          "id": "WSKEY-AUTOUPLOAD",
          "name": "自动上传WSKEY",
          "val": true,
          "type": "boolean",
          "desc": "启用后，重写抓取的WSKEY会自动发送给机器人"
        },
        {
          "id": "WSKEY_TG-BOTTOKEN",
          "name": "TG-BOTToken",
          "val": "",
          "type": "text",
          "desc": "填写Telegram机器人的Token，格式：123456:abcdefghjkl"
        },
        {
          "id": "WSKEY-TG-USERID",
          "name": "TG-chatID",
          "val": "",
          "type": "text",
          "desc": "填写TG-chatID，以数组形式保存[\"12345678\"]"
        },
        {
          "id": "wskeyList",
          "name": "WSKEY",
          "val": "",
          "type": "textarea",
          "rows": 10,
          "desc": "京东WSKEY"
        }
      ]
    }


  ]
}
